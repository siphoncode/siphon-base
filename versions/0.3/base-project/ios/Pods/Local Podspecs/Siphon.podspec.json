{
  "name": "Siphon",
  "version": "0.3",
  "summary": "Build React Native apps easily.",
  "authors": {
    "Siphon": "hello@getsiphon.com"
  },
  "source": {
    "git": "https://bitbucket.org/getsiphon/siphon-sdk-ios.git"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": [
    "SiphonSDK/Build/Products/Release-universal/SiphonSDKHeaders/SPAppViewController.h",
    "SiphonSDK/Build/Products/Release-universal/SiphonSDKHeaders/SPDevelopmentAppViewController.h"
  ],
  "vendored_libraries": "SiphonSDK/Build/Products/Release-universal/libSiphonSDK.a",
  "public_header_files": [
    "SiphonSDK/Build/Products/Release-universal/SiphonSDKHeaders/SPAppViewController.h",
    "SiphonSDK/Build/Products/Release-universal/SiphonSDKHeaders/SPDevelopmentAppViewController.h"
  ],
  "resource_bundles": {
    "SiphonResources": [
      "SiphonSDK/SiphonSDK/Supporting Files/{pre-header,header,sandbox-header}"
    ]
  },
  "dependencies": {
    "React": [
      "0.18.0"
    ],
    "SSZipArchive": [
      "0.3.3"
    ],
    "SocketRocket": [
      "0.4.2"
    ],
    "Mixpanel": [
      "2.9.0"
    ]
  }
}
